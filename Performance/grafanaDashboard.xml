          "format": "time_series",
          "rawSql": "USE PerfmonCollector\n\nSELECT\n  MachineName\n ,ISNULL(CounterDetails.InstanceName,'-') AS Instance\n ,CounterName AS Counter\n ,CONVERT(DATETIME, CONVERT(VARCHAR(19), CounterDateTime)) as \"time\"\n ,AVG(CounterData.CounterValue) AS 'metric'\n\nFROM CounterData\n INNER JOIN CounterDetails ON CounterData.CounterID = CounterDetails.CounterID\n INNER JOIN DisplayToID ON CounterData.GUID = DisplayToID.GUID\n\nWHERE\n CounterName IN ($Counters)\n AND MachineName IN ($Machines)\n AND ISNULL(CounterDetails.InstanceName,'-') IN ($Instance)\n\nGROUP BY MachineName,ISNULL(CounterDetails.InstanceName,'-'),CounterName,CONVERT(DATETIME, CONVERT(VARCHAR(19), CounterDateTime))\n\nORDER BY CONVERT(DATETIME, CONVERT(VARCHAR(19), CounterDateTime))\n \n\n\n\n",
          "refId": "A"
        }
